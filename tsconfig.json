{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "forceConsistentCasingInFileNames": false,
    "baseUrl": ".",
    "paths": {
      "$app/*": ["src/app/*"],
      "$core/*": ["src/core/*"],
      "$modules/*": ["src/modules/*"],
      "$styles/*": ["src/styles/*"],
      "@public/*": ["public/*"]
    }
  }
}
